<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.bit.approval.mapper.TestMapper">

    <select id="selectUser" resultType="com.project.bit.approval.domain.UserVO">
    select * from test_user
    </select>

    <select id="selectForm" resultType="com.project.bit.approval.domain.UserVO">
        select form from test_user where id = #{userId}
    </select>

</mapper>
